<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hello.com.eventratingapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".EventDashboardActivity" />
        <activity android:name=".CreateEventActivity" />
        <activity android:name=".CreateCompetitorActivity" />
        <activity android:name=".SearchEventActivity" />
        <activity android:name=".EventListActivity" />
        <activity android:name=".EventHomeActivity" />
        <activity android:name=".EventCompetitorListActivity" />
        <activity android:name=".EventCompetitorProfileActivity" />
        <activity android:name=".EventCompetitorProfileRateActivity" />
        <activity android:name=".WebViewActivity" />
        <activity android:name=".CreateEventRoundActivity" />
        <activity android:name=".EventRoundHomeActivity" />
        <activity android:name=".EventRoundDashboardActivity" />
        <activity android:name=".AddRoundCompetitorActivity" />
        <activity android:name=".BarChartActivity" />
        <activity android:name=".PieChartActivity" />
        <activity android:name=".RadarChartActivity"></activity>
    </application>

</manifest>