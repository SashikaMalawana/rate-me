<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hello.com.eventratingapp">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".EventDashboardActivity"></activity>
        <activity android:name=".CreateEventActivity"></activity>
        <activity android:name=".CreateCompetitorActivity"></activity>
        <activity android:name=".SearchEventActivity"></activity>
        <activity android:name=".EventListActivity"></activity>
        <activity android:name=".EventHomeActivity"></activity>
        <activity android:name=".EventCompetitorListActivity"></activity>
        <activity android:name=".EventCompetitorProfileActivity"></activity>
        <activity android:name=".EventCompetitorProfileRateActivity"></activity>
        <activity android:name=".WebViewActivity"></activity>
    </application>

</manifest>